@Book{StockerPCV,
  Title                    = {Plural and Conflicting Values},
  Author                   = {Michael Stocker},
  Publisher                = {Oxford University Press},
  Year                     = {1989},

  Address                  = {Oxford},

  Abstract                 = {Plural and conflicting values are often held to be conceptually problematic, threatening the very possibility of ethics, or at least rational ethics. Rejecting this view, Stocker first demonstrates why it is so important to understand the issues raised by plural and conflicting values, focusing on Aristotle's treatment of them. He then shows that plurality and conflict are commonplace and generally unproblematic features of our everyday choice and action, and that they do allow for a sound and rational ethics.}
}

